
.iphone_x{
	height: 30rpx;
}
.goBackHomePage {
	position: fixed;
	right: 7rpx;
	bottom: -5rpx;
	-webkit-transform: translateY(-50rpx);
	        transform: translateY(-50rpx);
}
page {
	height: 100%;
	background-color: #f2f2f2;
	width: 100%;
	margin: 0;
	font-family: "\5FAE\8F6F\96C5\9ED1", "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
}
.empty_link_view {
	height: 310rpx;
	width: 100%;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
}
.message_detail {
	width: 100%;
	background: #f2f2f2;
	box-sizing: border-box;
}
.message_cont {
	width: 100%;
	height: 100%;
	padding: 0 30rpx 40rpx;
	box-sizing: border-box;
	background: #f2f2f2;
}
.message_item {
	width: 100%;
	margin-bottom: 25rpx;
	padding-right: 20rpx;
}
.msg_send_time_view {
	text-align: center;
	font-size: 24rpx;
	margin: 40rpx 0;
}
.msg_send_time_text {
	color: #fff;
	background: #D2D2D2;
	padding: 4rpx 10rpx;
	border-radius: 4rpx;
	will-change: transform;
	max-width: 230rpx;
	text-align: center;
	margin: 0 auto;
}
.sned_msg_view {
	width: 100%;
	bottom: 0;
	border-top: 1px solid #efefef;
	background-color: #fff;
	box-sizing: border-box;
	position: fixed;
	z-index: 99;
}
.service_link_view {
	background: #FFFFFF;
	width: 90%;
	border-radius: 3px;
	will-change: transform;
	box-sizing: border-box;
	padding: 20rpx 30rpx;
	font-size: 28rpx;
	color: #333333;
	line-height: 42rpx;
	margin: 30rpx auto;
}
.service_link_cont {
	padding-bottom: 20rpx;
}
.service_link_title {
	margin-bottom: 20rpx;
}
.service_link_item {
	color: #4DAAF0;
	text-decoration: underline;
	margin-bottom: 6rpx;
}
.top_empty_view {
	height: 30rpx;
	width: 100%;
}
/* 优惠券隐藏btn */
.coupon_hide_btn {
	position: fixed;
	right: 10rpx;
	bottom: 250rpx;
	width: 90rpx;
	height: 100rpx;
	opacity: 0.9;
	background: url(http://cdn.haofang.net/static/uuminiapp/im/coupon/coupon_redbag.png) no-repeat 0 0;
	background-size: 100%;
	margin-bottom: 30rpx;
	text-align: center;
	vertical-align: middle;
	display: -webkit-box;
	display: flex;
	display: -webkit-flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.coupon_hide_btn_text {
	width: 100%;
	height: 18rpx;
	font-size: 18rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 37rpx;
	letter-spacing: 0px;
	color: #ffd800;
	text-align: center;
}
.coupon-success {
	width: 590rpx;
	background-color: #fef6e9;
	margin: 0 auto;
	text-align: left;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 50rpx;
	letter-spacing: 0px;
	color: #fe943e;
	padding: 25rpx 30rpx;
	margin-bottom: 20rpx;
}

