}
.input input{
	    width:100%;
        margin: auto 0;
	    height: 40rpx!important;
	    line-height: 40rpx!important;
	    box-sizing:border-box;
	    font-size:28rpx;
	    color:#666666;
	    border:0;
	    outline:none;
	    background-color:#F8F8F9;
}
.clear_btn{
	    width:82rpx;
	    height: 82rpx;
	    background: url(http://cdn.haofang.net/static/uuminiapp/search/clear.png) no-repeat center center;
	    background-size: 34rpx 34rpx;
	    -webkit-flex-shrink: 0;
	            flex-shrink: 0;
}
	/* 列表 */
.house_list{
	    padding: 0 40rpx;
	    box-sizing: border-box;
}
.house_item{
		font-size:32rpx;
		color:#101d36;
		border-bottom:1px solid #F5F5F5;
		height:112rpx;
		line-height:112rpx;
		position:relative;
		letter-spacing:1px;
		box-sizing:border-box;
		width: 100%;
}
.house_name{
	    text-align: left;
	    max-width: 75%;
	    text-overflow: ellipsis;
	    white-space: nowrap;
	    overflow: hidden;
        font-weight: normal;
        font-size: 32rpx;
        color: #101D36;
}
.house_addr{
	    text-align: right;
	    max-width: 25%;
	    text-overflow: ellipsis;
	    white-space: nowrap;
	    overflow: hidden;
	    color:#8e94a1;
	    -webkit-flex-shrink: 0;
	            flex-shrink: 0;
		font-weight: normal;
}
.clear_local{
	    width: 100%;
	    padding-right: 30rpx;
	    box-sizing: border-box;
	    height:100rpx;
	    line-height:100rpx;
	    text-align:center;
	    margin:0 auto;
	    font-size:24rpx;
	    color:#606060;
}
	/* 暂无搜索结果 */
.no_search_list{
		margin-top: 40rpx;
		line-height:60rpx;
		font-size:28rpx;
		color:#999999;
		text-align:center;
		width: 100%;
}

