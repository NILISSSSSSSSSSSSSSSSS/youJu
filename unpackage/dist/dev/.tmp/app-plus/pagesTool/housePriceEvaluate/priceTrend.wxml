<view><block wx:if="{{loading&&!noDataShow}}"><view class="loading_mask"></view></block><view class="top_view global-confirm-btn"><view class="top_price_view f_r_s"><text class="{{['top_price f_c_e',(!item.totalPrice&&item.totalPrice!=null)?'no_price':'']}}">{{!item.totalPrice&&item.totalPrice!=null?'价格面议':item.totalPrice}}</text><block wx:if="{{item.totalPrice&&item.totalPrice!=null}}"><text class="top_price_unit f_c_e">万</text></block><block wx:if="{{item.changePriceVal}}"><text class="f_c_e">{{"("+item.changePriceVal+"万 较上月"+(item.changePriceVal<0?'下跌':'上升')+item.changePriveBiLi+"%)"}}</text></block></view><view class="top_build_name">{{item.buildName}}</view><view class="f_r_s top_house_des"><block wx:if="{{item.room}}"><view class="top_house_room">{{item.room+"室"}}</view></block><block wx:if="{{item.area}}"><view class="top_house_area">{{item.area+"㎡"}}</view></block><block wx:if="{{item.currentFloor&&item.allFloor}}"><view class="top_house_floor">{{item.currentFloor+"/"+item.allFloor+"层"}}</view></block></view></view><block wx:if="{{!(!item.totalPrice&&item.totalPrice!=null)}}"><view class="price_trend_view"><view class="price_trend_title f_r_b">价格走势<view class="f_r_e"><view class="canvas_cityName f_r_e"><view class="canvas_point" style="background:#5786f2;"></view>{{''+buildName+''}}</view><view class="canvas_cityName f_r_e"><view class="canvas_point" style="background:#c2c2c2;"></view>{{''+regionName+''}}</view><view class="canvas_cityName f_r_e"><view class="canvas_point" style="background:#fbcc5b;"></view>{{''+cityName+''}}</view></view></view><view class="price_trend"><canvas style="width:100%;height:100%;transform:scale(1);" id="lineCanvas_price" canvas-id="lineCanvas_price"></canvas></view></view></block><block wx:if="{{!item.totalPrice&&item.totalPrice!=null}}"><view class="no_chart_view"></view></block><view class="expert_view"><village-expert vue-id="1" caseType="1" item="{{villageExpertItem}}" data-event-opts="{{[['^onPushEntrustChanged',[['onPushEntrustChanged']]]]}}" bind:onPushEntrustChanged="__e" bind:__l="__l"></village-expert></view><block wx:if="{{houseList.length>0}}"><view class="house_list_view"><view class="f_r_b house_list_title"><view>周边在售房源</view><block wx:if="{{houseList.length>3}}"><view class="f_r_e"><view class="look_more">查看更多</view><image class="right_arrow" src="https://zdzfapi.haofang.net/Public/wxApp/images/mine/arrow_right.png"></image></view></block></view><block wx:for="{{houseList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><er-house-list-item class="{{[(index+1==houseList.length)?'er_last_item':'']}}" vue-id="{{'2-'+index}}" item="{{item}}" bind:__l="__l"></er-house-list-item></block></block></view></block><block wx:if="{{bottomBtn&&!(!item.totalPrice&&item.totalPrice!=null)}}"><block><view class="empty_bottom"></view><view class="bootom_view f_r_b"><view data-event-opts="{{[['tap',[['wantDaiKaun',['$event']]]]]}}" class="dai_kuan_view f_c_c" bindtap="__e"><image class="dai_kuan_icon" src="http://cdn.haofang.net/static/uuminiapp/pageNewUi/registration/icon_daikuan.png"></image><view class="dai_kuan_text">我要贷款</view></view><view class="f_r_b want_view"><view data-event-opts="{{[['tap',[['wantSaleHouse',['$event']]]]]}}" class="want_buy global-confirm-btn-another" bindtap="__e">我要卖房</view><view data-event-opts="{{[['tap',[['wantLeaseHouse',['$event']]]]]}}" class="want_lease global-confirm-btn-right" bindtap="__e">我要出租</view></view></view></block></block><block wx:if="{{!loading&&noDataShow}}"><view class="no-data-box"><view class="no-img"></view><view data-event-opts="{{[['tap',[['gobackBtn',['$event']]]]]}}" class="goback-btn" bindtap="__e">换个条件试试</view></view></block></view>