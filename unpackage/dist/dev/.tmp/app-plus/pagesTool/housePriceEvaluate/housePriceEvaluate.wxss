
.canvas_cityName{
        position: relative;
        color: #101D36;
        font-size: 26rpx;
        font-weight: normal;
        width: 200rpx;
        text-align: right;
}
.canvas_point{
        width: 12rpx;
        height: 12rpx;
        margin: auto 12rpx auto 0;
        border-radius: 100%;
		will-change: transform;
        overflow: hidden;
        background: #fbcc5b;
}
.evaluate_top_view{
		position: relative;
}
.evaluate_top_view image{
		width: 100%;
		height: 322rpx;
}
.evaluate_top_cont{
		position: absolute;
		left: 0;
		top: 0;
		width: 100%;
		background: inherit;
		padding: 55rpx 0 0 40rpx;
		box-sizing: border-box;
		height: 100%;
		color: #FFFFFF;
}
.name_monthVal{
		line-height: 50rpx;
		text-align: left;
		font-size: 30rpx;
}
.top_point{
		width: 6rpx;
		height: 6rpx;
		background: #FFFFFF;
		overflow: hidden;
		margin: auto 15rpx;
		border-radius: 6rpx;
		will-change: transform;
}
.unit_price{
		font-size: 94rpx;
		font-weight: normal;
        color: #FFFFFF;
        margin-left: 0;
		margin-top: 10rpx;
}
.unit{
		font-size: 30rpx;
		font-weight: normal;
}
.price_val{
		position: relative;
		bottom: -4rpx;
		margin-right: 4rpx;
}
.evaluate_cont{
		padding: 0 25rpx;
		box-sizing: border-box;
		margin-top: -47rpx;
		position: relative;
		z-index: 99;
}
.num_change_view{
		width:100%;
		height:180rpx;
		background:#fff;
		box-sizing:border-box;
		box-shadow:0rpx 10rpx 50rpx #ececec;
}
.num_change_item{
		width: 100%;
		height: 100%;
		position: relative;
		padding-left: 36rpx;
		box-sizing: border-box;
}
.num_change_item:last-child::after{
		content: "";
		display: none;
}
.num_change_item::after{
		content: "";
		position: absolute;
		right: 0;
		top: 50%;
		width: 2rpx;
		height: 84rpx;
		-webkit-transform: translateY(-50%);
		        transform: translateY(-50%);
		background: #ECECEC;
}
.num_change_title{
		font-size: 26rpx;
		color: #89909D;
		line-height: 50rpx;
		margin-top: 25rpx;
}
.num_change_num{
		line-height: 48rpx;
		margin-top: 10rpx;
		font-size: 44rpx;
		font-weight: bold;
		color: #101D36;
}
.lineCanvas{
		width: 100%;
		height: 100%;
}
.canvas_view{
		height:200px;
		padding-top:27rpx;
		position:relative;
		margin-left: -20rpx;
        margin-bottom: 20rpx;
}
.canvas_title{
		width:100%;
		height:57rpx;
		font-weight:bold;
		color:#101d36;
		font-size:34rpx;
		line-height:57rpx;
		box-sizing:border-box;
}
.canvas_cont{
		width:100%;
		background:#fff;
		margin-top:43rpx;
		padding: 0 15rpx;
		box-sizing:border-box;
}
.canvas_des{
		padding: 0 15rpx;
		width:100%;
		box-sizing:border-box;
		font-size:22rpx;
		color:#8e94a1;
}
.evaluate_btn_view{
		width: 100%;
		padding: 0 15rpx;
		box-sizing: border-box;
}
.evaluate_btn{
		width:100%;
		height:100rpx;
		line-height:100rpx;
		background:#ff6f2e;
		background:-webkit-linear-gradient(left, #fe8e0d , #ff6f2e);
		color:#fff;
		text-align:center;
		font-size:32rpx;
		display:-webkit-box;
		display:-webkit-flex;
		display:flex;
		-webkit-box-pack:center;
		-webkit-justify-content:center;
		        justify-content:center;
		-webkit-box-align:center;
		-webkit-align-items:center;
		        align-items:center;
		margin-top:70rpx;
		border-radius:10rpx;
		will-change: transform;
		box-sizing:border-box;
}
.empty_view{
		height: 60rpx;
		width: 100%;
}

