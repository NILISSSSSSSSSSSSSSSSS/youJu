
.remove_btn{
    width: 120rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    border-radius: 8rpx;
    overflow: hidden;
    margin: auto 0;
    background-color: #ab7f2e;
    font-size: 30rpx;
    color: #ffffff;
}
/*暂无数据*/
.no_data{
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: #ffffff;
    z-index: 200;
    padding: 40rpx;
    box-sizing: border-box;
}
.no_data_img{
    width: 336rpx;
    height: 200rpx;
    margin: 0 auto;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
}
.no_data_text{
    text-align: center;
    margin-top: 10rpx;
}
page{
    width: 100%;
    background-color: #ffffff;
}
.console_view{
    width: 100%;
    background-color: #ffffff;
    padding: 30rpx 40rpx 40rpx;
    box-sizing: border-box;
}
.all_choose{
    height: 120rpx;
    line-height: 120rpx;
    position: fixed;
    z-index: 100;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    background-color: #ffffff;
    box-shadow: 0px -1rpx 32rpx 0rpx  rgba(97, 97, 97, 0.11);
    padding: 0 40rpx;
    box-sizing: border-box;
}
.list_view{
    width: 100%;
}
.console_item{
    margin-top: 30rpx;
}
.item_title{
    margin: auto 0 auto 8rpx;
    color: #2C405A;
    font-size: 32rpx;
}
.item_cont{
    color: #000000;
    margin-top: 10rpx;
    line-height: 40rpx;
    font-size: 34rpx;
    -webkit-flex-wrap: wrap;
            flex-wrap: wrap;
    word-break: break-word;
}
.bottom_empty{
    width: 100%;
    height: 120rpx;
}
.all_choose_icon{
    width: 46rpx;
    height: 46rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: auto 10rpx auto 0;
}

