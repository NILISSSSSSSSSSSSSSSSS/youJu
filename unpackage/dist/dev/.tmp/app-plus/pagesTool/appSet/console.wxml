<view class="console_view"><block wx:if="{{checkList&&checkList.length===0}}"><view class="no_data f_c_c"><image class="no_data_img" src="/static/app-plus/images/console_no_data.png"></image><view class="no_data_text">暂无日志</view></view></block><checkbox-group data-event-opts="{{[['change',[['checkBoxChange',['$event']]]]]}}" hidden="{{!(checkList&&checkList.length>0)}}" class="f_c_s list_view" bindchange="__e"><block wx:for="{{checkList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><label class="f_c_s console_item"><view class="f_r_s"><checkbox value="{{item.id}}" checked="{{chooseIdMap[item.id]}}" color="#ab7f2e"></checkbox><view class="f_c_c item_title">{{index+1+". "+item.name}}</view></view><text data-event-opts="{{[['longtap',[['longtap',['$0'],[[['checkList','id',item.id,'value']]]]]]]}}" class="f_r_s item_cont" bindlongtap="__e">{{item.key+": "+item.value}}</text></label></block></checkbox-group><view class="bottom_empty"></view><view class="f_r_b all_choose"><view data-event-opts="{{[['tap',[['allChoose',['$event']]]]]}}" class="f_r_s" bindtap="__e"><image class="all_choose_icon" src="{{allChooseMap[isAllChoose].url}}"></image><view>{{allChooseMap[isAllChoose].text}}</view></view><view class="f_r_e"><block wx:if="{{showDeal}}"><view data-event-opts="{{[['tap',[['removeItem',['$event']]]]]}}" class="remove_btn" bindtap="__e">删除</view></block></view></view></view>