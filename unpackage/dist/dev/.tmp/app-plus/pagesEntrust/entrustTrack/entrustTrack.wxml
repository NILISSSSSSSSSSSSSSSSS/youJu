<view><block wx:if="{{isRequesting}}"><scroll-loading vue-id="1" isScrollIng="{{true}}" bind:__l="__l"></scroll-loading></block><block wx:if="{{!isRequesting&&trackInfo.houseDes}}"><view><view class="f_r_b entrust_top"><view class="f_r_s"><view class="entrust_casetype f_c_c">{{caseType==1?'出售':'出租'}}</view><view class="entrust_create_time f_c_c">{{trackInfo.createDate}}</view></view><view data-event-opts="{{[['tap',[['onlineService',['$event']]]]]}}" class="f_r_e" bindtap="__e"><image class="entrust_service_img" mode="aspectFill" src="https://zdzfapi.haofang.net/PublicC/images/service_new.png"></image><view class="f_c_c entrust_service_text">客服</view></view></view><view class="f_r_s track_house_view"><view class="track_house_img f_r_c"><image mode="aspectFill" src="{{trackInfo.houseImg}}" data-event-opts="{{[['error',[['houseImgError',['$event']]]]]}}" binderror="__e"></image></view><view class="track_house_info"><view class="track_house_des">{{trackInfo.houseDes}}</view><view class="f_r_s track_house_floor"><block wx:if="{{trackInfo.houseArea}}"><view>{{"面积: "+trackInfo.houseArea+"㎡"}}</view></block><block wx:if="{{trackInfo.houseFloor&&trackInfo.houseFloors}}"><view>{{"楼层: "+trackInfo.houseFloor+"/"+trackInfo.houseFloors}}</view></block></view><view class="track_house_botttom f_r_b"><block wx:if="{{trackInfo.housePrice}}"><view class="track_house_price main_color">{{trackInfo.housePrice+trackInfo.housePriceUnit}}</view></block><block wx:if="{{!trackInfo.housePrice}}"><view class="no_price_view"></view></block><view class="f_r_e track_push_look_view"><view class="track_push_num">{{"推送 "+trackInfo.tuisongNumAll}}</view><view class="track_look_num">{{"带看 "+trackInfo.daikanNumAll}}</view></view></view></view></view><view class="track_list"><block wx:for="{{trackInfo.trackList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:for="{{item.trackInfos}}" wx:for-item="info" wx:for-index="infoIdnex" wx:key="infoIdnex"><view class="track_list_item"><view class="track_list_item_info"><view class="track_list_date">{{item.date+" "+info.hour}}</view><view class="track_list_title">{{"["+info.trackTitle+"] "+info.item}}</view></view></view></block></block></block></view></view></block><login-view vue-id="2" bind:__l="__l"></login-view></view>