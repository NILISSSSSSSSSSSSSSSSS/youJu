
page{
	background: #F2F2F2;
	width: 100%;
	overflow-x: hidden;
}
.trust_list_view{
	background: #F2F2F2;
	width: 100%;
	padding: 0 20rpx;
	box-sizing: border-box;
	overflow-x: hidden;
}
.success_get_trust_view{
	width: 100%;
	padding: 30rpx 20rpx;
	box-sizing: border-box;
}
.success_get_trust{
	width: 100%;
	height:60rpx;
	background-color:#fef6e9;
	border-radius:30rpx;
	will-change: transform;
	color:#f68000;
	font-size:26rpx;
	text-align:center;
	line-height:60rpx;
}
.top_empty_view{
	height: 30rpx;
	width: 100%;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
}
.no_more_data{
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size:26rpx;
    color:#666666;
}
.loading_view{
    -webkit-transform-origin: 0px 0px 0px;
            transform-origin: 0px 0px 0px;
    opacity: 1;
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
    height:40px;
    line-height:40px;
    font-size:12px;
    text-align:center;
    margin-top: 10rpx;
}
.pullUpIcon{
    display:inline-block;
    height:15px;
    width:15px;
    border-radius:100%;
	will-change: transform;
    margin:auto 6px;
    border:2px solid #666;
    border-bottom-color:transparent;
    vertical-align:middle;
    -webkit-animation:loading 1s linear infinite;
            animation:loading 1s linear infinite;
}
@-webkit-keyframes loading {
0% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
50% {
        -webkit-transform: rotate(180deg);
                transform: rotate(180deg);
}
100% {
        -webkit-transform: rotate(360deg);
                transform: rotate(360deg);
}
}
@keyframes loading {
0% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
50% {
        -webkit-transform: rotate(180deg);
                transform: rotate(180deg);
}
100% {
        -webkit-transform: rotate(360deg);
                transform: rotate(360deg);
}
}
