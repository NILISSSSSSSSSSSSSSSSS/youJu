
page{
		height: 100%;
}
::-webkit-scrollbar{width:0px;height: 0}
.push_msg_view{
		width: 100%;
		height: 100%;
		background: #FFFFFF;
		overflow: hidden;
}
.finish_push{
		position:absolute;
		top:20rpx;
		right:20rpx;
		font-size:32rpx;
}
	/* 进度 */
.circl_progress{
		position:relative;
		width:300rpx;
		height:300rpx;
		margin:60rpx auto;
}
.agent_num_view{
		position:absolute;
		top:80rpx;
		margin:auto;
		width:100%;
		text-align:center;
}
.agent_num_val{
		font-size:20rpx;
		color:#4daaf0;
}
.agent_num{
		font-size:60rpx;
}
.circle_dot{
		position: absolute;
		left: 0;
		right: 0;
		margin: auto;
		top: 0;
		background: url(http://cdn.haofang.net/static/uuminiapp/new_images/agent_gif.png) center no-repeat;
		background-size: 100% 100%;
		width: 300rpx;
		height:300rpx;
		-webkit-animation: circleRotate 14s infinite;
		        animation: circleRotate 14s infinite;
		-webkit-transition-timing-function: linear;
		        transition-timing-function: linear;
}
@-webkit-keyframes circleRotate {
from {
            -webkit-transform: rotate(0deg);
                    transform: rotate(0deg);
}
to {
			-webkit-transform: rotate(360deg);
			        transform: rotate(360deg);
}
}
@keyframes circleRotate {
from {
            -webkit-transform: rotate(0deg);
                    transform: rotate(0deg);
}
to {
			-webkit-transform: rotate(360deg);
			        transform: rotate(360deg);
}
}
	/* 等待经纪人 */
.wait_title_view{
		-webkit-box-align:center;
		-webkit-align-items:center;
		        align-items:center;
		position:relative;
		left:0;
		right:0;
		margin:auto;
		height:40rpx;
		margin-bottom:40rpx;
		margin-top:40rpx;
}
.wait_title_line{
		position:relative;
		width:20%;
		height:100%;
}
.wait_title_line::after{
		content: '';
		position: absolute;
		left: 0;
		right: 0;
		margin: auto;
		bottom: 50%;
		z-index: 15;
		display: block;
		height: 1px;
		width: 100%;
		background-color: #e4e4e4;
}
.wait_title_text{
		width:38%;
		height:100%;
		line-height:40rpx;
		text-align:center;
		font-size:32rpx;
		color:#808080;
}
.agent_list_view{
		width:100%;
		padding:0 50rpx;
		box-sizing:border-box;
		margin-bottom:40rpx;
}
.swiper-wrap-box{
		position: relative;
		z-index: 5;
		width: 110rpx;
		height: 110rpx;
		border: 1px solid #e1e1e1;
		border-radius: 100%;
		overflow: hiden;
}
.sw-sli{
		width: 110rpx;
		height: 110rpx;
		border-radius: 100%;
		overflow: hiden;
}
.agent-image{
		position: relative;
		width: 110rpx;
		height: 110rpx;
		z-index: -5;
}
	/* 砸蛋 */
.footer-box{
