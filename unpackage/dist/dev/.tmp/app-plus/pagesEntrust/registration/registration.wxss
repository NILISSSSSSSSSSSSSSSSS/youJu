	/* 底部 */
.filter_buy_one{
		text-align:left;
		position:fixed;
		bottom:166rpx;
		height:50rpx;
		line-height:99rpx;
		z-index:100;
		background:#FFFFFF;
		left:0;
		padding-left:40rpx;
		box-sizing:border-box;
		font-size:28rpx;
		color:#fe8615;
		width:100%;
}
.next{
		position:fixed;
		bottom:0;
		left:0;
		width:100%;
		padding:40rpx;
		box-sizing:border-box;
		z-index:99;
		background-color:#FFFFFF;
}
.next_text{
		width:100%;
		height:90rpx;
		line-height:90rpx;
		text-align:center;
		font-size:36rpx;
		background:#ff6f2e;
		background:-webkit-linear-gradient(left, #fe8e0d, #ff6f2e);
		color:#FFFFFF;
		border-radius:8rpx;
		will-change: transform;
		box-sizing:border-box;
}
	/* 选择服务方式 */
.choose_service_type{
		width: 100%;
		margin-top: 48rpx;
}
.choose_service_type .choose_service_item:first-child{
		margin-right: 23rpx;
}
.choose_service_type .choose_service_active{
		background-color:#FFFFFF;
		color:#ab7f2e;
		border:1rpx solid #ab7f2e;
}
.choose_service_item{
		width:100%;
		height:120rpx;
		line-height:120rpx;
		text-align:center;
		color:#101d36;
		font-size:28rpx;
		background-color:#f1f3f6;
		border-radius:12rpx;
		will-change: transform;
		margin:auto 0;
		box-sizing:border-box;
		overflow:hidden;
}
	/* 遮罩弹出 */
#g-mask{
		width: 100%;
		height: 100%;
		position: fixed;
		top: 0;
		z-index: 999;
}
.g-mask-box{
		width: 100%;
		height: 100%;
		background-color: #000000;
		opacity: 0.5;
}
.g-mask-content{
		width: 630rpx;
		height: auto;
		background-color: #ffffff;
		border-radius: 10rpx;
		margin: auto;
		z-index: 999;
		padding-bottom: 70rpx;
		position: absolute;
		top: 20%;
		left: 0;
		right: 0;
}
.g-mask-center{
		width: 530rpx;
		height: 100%;
		margin: 0 auto;
}
.g-mask-icon{
		width: 102rpx;
		height: 129rpx;
		display: block;
		margin: 0 auto;
		padding-top: 50rpx;
}
.g-mask-title{
		display: block;
		text-align: center;
		color: #333333;
		font-size: 36rpx;
		padding: 20rpx 0 18rpx 0;
		font-weight: bold;
}
.g-mask-text{
		display: block;
		height: auto;
		font-size: 28rpx;
		color: #666666;
}
.g-mask-info{
		padding-top: 50rpx;
		font-size: 24rpx;
		color: #666666;
}
.s-orange{
		color: #ab7f2e;
}
.g-mask-btn{
		width: 530rpx;
		height: 80rpx;
		line-height: 80rpx;
		color: #ffffff;
		font-size: 32rpx;
		background-image: -webkit-linear-gradient(right,
		#fe8e0d 0%,
		#ff6f2e 100%),
		-webkit-linear-gradient(
				#919191,
				#919191);
		background-image: linear-gradient(-90deg,
		#fe8e0d 0%,
		#ff6f2e 100%),
		linear-gradient(
				#919191,
				#919191);
		background-blend-mode: normal,
		normal;
		border-radius: 40rpx;
		text-align: center;
		margin: 39rpx auto 0 auto;
}

