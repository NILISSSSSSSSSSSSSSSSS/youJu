.line-w{
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    top: 45rpx;
    width: 70%;
    height: 2rpx;
    background: #b7b7b7;
}
.line-w-a{
    position: absolute;
    height: 1rpx;
    left: 0;
    top: 25rpx;
    background: green;
}
.progress-1 .condition-box .line-w-a{
    width:0%;
    background: #4daaf0;
}
.progress-2 .condition-box .line-w-a{
    width:35%;
    background: #4daaf0;
}
.progress-3 .condition-box .line-w-a{
    width:65%;
    background: #4daaf0;
}
.progress-4 .condition-box .line-w-a{
    width:95%;
    background: #4daaf0;
}
.condition{
    position: relative;
    z-index: 3;
}
.progress-1 .condition:nth-of-type(1),
.progress-2 .condition:nth-of-type(1),
.progress-2 .condition:nth-of-type(2),
.progress-3 .condition:nth-of-type(1),
.progress-3 .condition:nth-of-type(2),
.progress-3 .condition:nth-of-type(3),
.progress-4 .condition:nth-of-type(1),
.progress-4 .condition:nth-of-type(2),
.progress-4 .condition:nth-of-type(3),
.progress-4 .condition:nth-of-type(4){
    color:#4daaf0;
}
.manager-progress .path-line::after{
    content: '';
    position:absolute;
    display: block;
    top:0rpx;
    left: 0;
    width:100%;
    margin: 0 25rpx;
    height: 2px;
    background: #b7b7b7;
    -webkit-transform: scale(1,0.5);
            transform: scale(1,0.5);
    -webkit-transform-origin: top center;
            transform-origin: top center;
}
/* 屏蔽弹窗 */
.to-wrap{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
}
.tost-box{
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
    z-index: 120;
    width: 600rpx;
    height: 320rpx;
    background: #fff;
    border-radius: 16rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    overflow: hidden;
}
.tost-title{
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-bottom:2rpx solid #e7e7e7;
}
.tost-txt{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    padding:10rpx 55rpx;
    line-height: 50rpx;
    font-size: 30rpx;
    color: #333333;
    text-align: center;
}
.toat-btn{
    width: 100%;
    height: 90rpx;
    font-size: 32rpx;
    text-align: center;
    line-height: 90rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
}
.toat-btn .recall{
    height: 75rpx;
    line-height: 75rpx;
    width: 35%;
    background: #ab7f2e;
    color: white;
    border-radius: 16rpx;
    margin-right:40rpx;
}
.toat-btn .pull{
    width: 35%;
    height: 75rpx;
    line-height: 75rpx;
    color: #ab7f2e;
    border: 2rpx solid #ab7f2e;
    border-radius: 16rpx;
    margin-left:40rpx;
}
