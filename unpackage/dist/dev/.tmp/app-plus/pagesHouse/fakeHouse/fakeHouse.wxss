
.reason_title{
	width:100%;
	height:70rpx;
	line-height:70rpx;
	background:#f8f8f8;
	font-size:28rpx;
	color:#999999;
	padding-left:40rpx;
	box-sizing:border-box;
}
.reason_cont{
	width:100%;
	padding-left:40rpx;
	box-sizing:border-box;
	border-top:2rpx solid #e9e9e9;
	margin-bottom:40rpx;
}
.reason_item{
	position:relative;
	width:100%;
	height:109rpx;
	font-size:32rpx;
	color:#101D36;
	line-height:109rpx;
	border-bottom:2rpx solid #e9e9e9;
}
.reason_cont .reason_item_active{
	color:#ab7f2e;
}
.reason_cont .reason_item_active::after{
	position: absolute;
	top: 0;
	bottom: 0;
	margin: auto;
	right: 26rpx;
	content: "";
	width: 33rpx;
	height: 24rpx;
	background: url('http://cdn.haofang.net/static/kdbweb/zdzfminiapp/zdzfPlatform/newUiStyle/check_icon.png') center center no-repeat;
	background-size:100%;
}
.reason_des{
	width:100%;
	padding:40rpx;
	box-sizing:border-box;
	color:#aaa;
	line-height:1.5;
	font-size:24rpx;
}
.look_detail{
	font-size:24rpx;
	text-align:center;
	color:#25b764;
	display:inline-block;
	margin-left:20rpx;
	font-weight:bold;
}
.empty_view{
	height: 140rpx;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	width: 100%;
}
.submit_view{
	position:fixed;
	left:0;
	right:0;
	bottom:45rpx;
	width: 100%;
	height: 90rpx;
	padding: 0 40rpx;
	box-sizing: border-box;
}
.submit{
	height: 100%;
	width: 100%;
	line-height:90rpx;
	font-size:34rpx;
	color:#ffffff;
	background:-webkit-linear-gradient(left, rgb(255,111,46) , rgb(254,142,13));
	border-radius:6rpx;
	text-align:center;
}

