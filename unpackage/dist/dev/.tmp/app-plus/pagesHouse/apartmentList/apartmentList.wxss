
page{
        background-color: #FFFFFF;
}
.top_banner_view{
		margin:40rpx 0 32rpx;
		height:190rpx;
		border-radius: 14rpx;
		box-sizing: border-box;
		overflow: hidden;
}
.top_banner_img{
		width: 100%;
		height: 100%;
}
.top_view{
        z-index: 999;
        position: fixed;
        top: 0;
        left: 50%;
        -webkit-transform: translateX(-50%);
                transform: translateX(-50%);
        width: 100%;
        background:#ffffff;
		margin: 0 auto;
}

.bottom_empty_view{
        height: 50rpx;
}
.active{
        overflow: hidden;
}
.er_house_list_cont{
        width: 100%;
        height: 100%;
        background: #FFFFFF;
}
.er_house_list_view{
        width: 100%;
        padding-left: 40rpx;
		padding-right: 40rpx;
        background: #FFFFFF;
        box-sizing: border-box;
}
.loading_view{
        -webkit-transform-origin: 0px 0px 0px;
                transform-origin: 0px 0px 0px;
        opacity: 1;
        -webkit-transform: scale(1, 1);
                transform: scale(1, 1);
        height:40px;
        line-height:40px;
        font-size:12px;
        text-align:center;
        margin-top: 10rpx;
}
.pullUpIcon{
        display:inline-block;
        height:15px;
        width:15px;
        border-radius:100%;
        margin:auto 6px;
        border:2px solid #666;
        border-bottom-color:transparent;
        vertical-align:middle;
        -webkit-animation:loading 1s linear infinite;
                animation:loading 1s linear infinite;
}
@-webkit-keyframes loading {
0% {
            -webkit-transform: rotate(0deg);
                    transform: rotate(0deg);
}
50% {
            -webkit-transform: rotate(180deg);
                    transform: rotate(180deg);
}
100% {
            -webkit-transform: rotate(360deg);
                    transform: rotate(360deg);
}
}
@keyframes loading {
0% {
            -webkit-transform: rotate(0deg);
                    transform: rotate(0deg);
}
50% {
            -webkit-transform: rotate(180deg);
                    transform: rotate(180deg);
}
100% {
            -webkit-transform: rotate(360deg);
                    transform: rotate(360deg);
}
}
.no_more_data{
        height: 80rpx;
        line-height: 80rpx;
        text-align: center;
        font-size:26rpx;
        color:#666666;
}
.no_house_list {
        width: 100%;
        position: relative;
        top: 200rpx;
}
.no-data-img {
        margin: 0 auto;
        width: 250rpx;
        height: 250rpx;
}
.no-txt {
        width: 100%;
        text-align: center;
        font-size: 32rpx;
        color: #999;
}

