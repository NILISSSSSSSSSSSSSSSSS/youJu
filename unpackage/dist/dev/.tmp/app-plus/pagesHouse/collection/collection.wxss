.no_collect_view{
		position: fixed;
		left: 0;
		top: 0;
		width: 100%;
		height: 100%;
		background: #FFFFFF;
		z-index: 99;
}
.no_collect_img{
		width:283rpx;
		height:287rpx;
		-webkit-flex-shrink: 0;
		        flex-shrink: 0;
		margin: 0 auto;
}
.no_collect_text{
		font-size:28rpx;
		color:#999;
		text-align:center;
		margin:20rpx auto 0;
}
.collect_history_list_view{
		margin-top: -20rpx;
}
.collect_history_list_view .collect_history_time:first-child{
		padding-top: 0;
}
.collect_history_time{
		padding:22rpx 0 0 0;
		font-size:20rpx;
		color:#999;
		box-sizing: border-box;
		margin-bottom:-17rpx;
		position: relative;
		z-index: 99;
		text-align: left;
}
.collect_view{
		padding: 0 40rpx;
		box-sizing: border-box;
}
	/* 经纪人 */
.collect_title{
		padding:50rpx 0 50rpx;
		font-size:36rpx;
		font-weight:bold;
		line-height:1;
		text-align: left;
}
.collect_agent_list{
		-webkit-box-align:start;
		-webkit-align-items:start;
		        align-items:start;
		overflow-x:auto;
		-webkit-overflow-scrolling:touch;
		padding-bottom:20rpx;
		margin-bottom:-20rpx;
		margin-top:-20rpx;
		padding-top:20rpx;
		overflow-y:auto;
}
.collect_history_list{
}
.collect_agent_item{
		width:200rpx;
		box-shadow:0rpx 0rpx 20rpx 0rpx rgba(194, 194, 194, 0.3);
		border-radius:6rpx;
		border:solid 1rpx #f2f2f2;
		padding-top:17rpx;
		box-sizing:border-box;
		margin-right:18rpx;
		-webkit-flex-shrink:0;
		        flex-shrink:0;
}
.collect_agent_img{
		position:relative;
		width:120rpx;
		height:120rpx;
		background:url(http://cdn.haofang.net/static/uuweb/buildingBook/housingLongFigure/default_head.png) center center no-repeat;
		background-size:100% 100%;
		-webkit-flex-shrink: 0;
		        flex-shrink: 0;
		border-radius: 120rpx;
		margin: 10rpx auto 25rpx;
		overflow: hidden;
        will-change: transform;
}
.collect_agent_img image{
		width: 100%;
		height: 100%;
        border-radius: 100%;
		margin: auto;
}
.collect_agent_name{
		color:#333;
		font-size:30rpx;
		margin-bottom:13rpx;
		font-weight:bold;
		white-space:nowrap;
		overflow:hidden;
		text-overflow:ellipsis;
		width:100%;
		text-align:center;
}
.collect_agent_join{
		color:#597192;
		font-size:22rpx;
		text-align: center;
		margin-bottom: 20rpx;
}
.collect_agent_join::after{
		content: '';
		position: relative;
		display: inline-block;
		top:-1rpx;
		width: 10rpx;
		height: 16rpx;
		margin-left: 8rpx;
		vertical-align: middle;
		background: url(http://cdn.haofang.net/static/uuminiapp/focusCollect/left_arrow.png) center center no-repeat;
		background-size: 100% 100%;
}

