
page{
		background: #f2f2f2;
}
.room_btn_scroll{
		background: #FFFFFF;
		overflow-x: auto;
		border-bottom:1px solid #F5F5F5;
		white-space:nowrap;
		box-sizing: border-box;
}
.room_btn_scroll .room_btn_active{
		color:#ab7f2e;
		border-color:#ab7f2e;
}
.room_btn_item{
		height:96rpx;
		line-height:96rpx;
		text-align:center;
		font-size:31rpx;
		color:#101D36;
		border-bottom:2rpx solid #ffffff;
		padding:0 10rpx;
		margin: 0 30rpx;
		-webkit-flex-shrink: 0;
		        flex-shrink: 0;
		box-sizing: border-box;
}
.room_cont{
		padding-top: 20rpx;
}
.room_cont_item{
		margin-bottom: 20rpx;
		background: #FFFFFF;
}
.wait_house{
		color:#4daaf0;
		height:80rpx;
		line-height:80rpx;
		border-top:1px solid #F5F5F5;
		border-bottom:1px solid #F5F5F5;
		font-size:32rpx;
		padding-left:40rpx;
		box-sizing: border-box;
}
.selling_house{
		color:#1bc964;
		height:80rpx;
		line-height:80rpx;
		border-top:1px solid #F5F5F5;
		border-bottom:1px solid #F5F5F5;
		font-size:32rpx;
		padding-left:40rpx;
		box-sizing: border-box;
}
.soldOut_house{
		color:#808080;
		height:80rpx;
		line-height:80rpx;
		border-top:1px solid #F5F5F5;
		border-bottom:1px solid #F5F5F5;
		font-size:32rpx;
		padding-left:40rpx;
		box-sizing: border-box;
}
.room_cont_des{
		font-size:28rpx;
		color:#101D36;
		padding-top:30rpx;
		padding-left: 40rpx;
		box-sizing: border-box;
}
.room_cont_des>view{
        margin: auto 30rpx auto 0;
}
.room_cont_img{
		padding: 40rpx;
		box-sizing: border-box;
		height: 480rpx;
		border-bottom: 1rpx solid #F5F5F5;
		background:url(http://cdn.haofang.net/static/wxPlusApp/yjk/house/house_list_default.png) no-repeat center center;
		background-size:80%;
		background-color:#FFFFFF;
}
.room_cont_img image{
		width: 100%;
		height: 100%;
}
	/* 查看大图 */
.max_img_view{
		position: fixed;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
        overflow: hidden;
		background: #000000;
}
.max_img_swiper{
		position: fixed;
		top: 50%;
        -webkit-transform: translateY(-50%);
                transform: translateY(-50%);
		left: 0;
		right: 0;
		bottom: 0;
		height: 100%;
}
.max_img_swiper_item{
		width: 100%;
		/* position: relative; */
}
.max_img_des{
		color:#fff;
		font-size:26rpx;
		background:#000000;
		border:1px solid #ffffff;
		padding:9rpx 12rpx;
		border-radius:6rpx;
		will-change: transform;
		line-height:1;
		position:absolute;
		bottom:152rpx;
		left:32rpx;
}
.max_img_url{
		width: 100%;
}
.max_img_btn{
		position: fixed;
		bottom: 0;
		left: 0;
		width: 100%;
		height:120rpx;
		border-top:1px solid #2e2e2e;
		background:#191919;
		padding-right: 40rpx;
		box-sizing: border-box;
}
.max_img_btn_text{
		width:210rpx;
		height:64rpx;
		line-height:64rpx;
		text-align:center;
		border:1px solid #fefefe;
		border-radius:32rpx;
		will-change: transform;
		color:#fefefe;
		font-size:32rpx;
		margin:auto 0;
}

