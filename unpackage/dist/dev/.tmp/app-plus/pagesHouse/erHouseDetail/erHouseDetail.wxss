
page {
  background: #FFFFFF;
}
.er_house_detail {
  overflow-x: hidden;
}
.near_map_item {
  height: 25rpx;
}
.position_near {
  box-shadow: 0rpx 9rpx 32rpx -10rpx rgba(20, 20, 20, 0.07);
  margin-bottom: 10rpx;
}
.near_house {
  margin-bottom: 20rpx;
}
.look_more {
  color: #7A8190;
  font-size: 24rpx;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  position: relative;
  line-height: 54rpx;
  margin: 57rpx 0 6rpx;
  margin-right: 40rpx;
  padding-right: 30rpx;
}
.look_more::after {
  position: absolute;
  content: '';
  width: 15rpx;
  height: 24rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  margin: auto;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  right: 0;
  background: url(http://cdn.haofang.net/static/uuminiapp/index/l-jtt.png) center center no-repeat;
  background-size: 100% 100%;
}


.bottom_btn_empty {
  height: 140rpx;
}
.x_bottom_empty {
  height: 180rpx !important;
}
.cont_empty_view {
  height: 40rpx;
}

/*撩客弹窗*/
.liaoke_tips_view {
  position: fixed;
  bottom: 140rpx;
  left: 20rpx;
  z-index: 99999;
  background: rgba(0, 0, 0, 0.5);
  width: 460rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.liaoke_tips_cont {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.liaoke_tips_text {
  font-size: 29rpx;
  color: #ffffff;
  text-align: left;
}
.liaoke_tips_del {
  width: 28rpx;
  height: 28rpx;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  margin: 7rpx 0 auto 10rpx;
}
.liaoke_tips_jiao {
  width: 0;
  height: 0;
  border-left: 12rpx solid transparent;
  border-right: 12rpx solid transparent;
  border-top: 20rpx solid rgba(0, 0, 0, 0.5);
  position: absolute;
  bottom: -20rpx;
  left: 56rpx;
}

