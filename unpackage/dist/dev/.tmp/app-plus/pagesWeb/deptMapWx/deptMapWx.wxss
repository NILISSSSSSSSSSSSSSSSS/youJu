
page{
    width: 100%;
    height: 100%;
}
.dept_map_view{
    width: 100%;
    height: 100%;
}
.dept_map{
    width: 100%;
    height: 100%;
}
.back_location{
    position: fixed;
    left: 40rpx;
    bottom: 60rpx;
    width: 80rpx;
    height: 80rpx;
}
.top_input{
    position: fixed;
    text-align: left;
    left: 30rpx;
    top: 30rpx;
    right: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    box-sizing: border-box;
    background-color: rgba(255,255,255,.9);
    box-shadow: 0 2rpx 20rpx rgba(0,0,0,.15);
    border-radius: 6rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.top_city_name{
    color: #101d36;
    font-size: 30rpx;
    display: inline-block;
    white-space: nowrap;
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
}
.top_line{
    display: inline-block;
    margin: auto 30rpx;
    width: 3rpx;
    height: 30rpx;
    background-color: #dddddd;
}
.top_search_icon{
    height: 26rpx;
    width: 26rpx;
    margin: auto 0;
    display: inline-block;
}
.top_clear_icon{
    position: absolute;
    top: 23.5rpx;
    height: 35rpx;
    width: 35rpx;
    right: 30rpx;
}
.top_input_pla{
    color: #bbbbbb;
    font-size: 30rpx;
    margin-left: 20rpx;
    height: 80rpx;
    line-height: 80rpx;
}
.top_input_val{
    color: #101d36;
    font-size: 30rpx;
    margin-left: 20rpx;
    height: 80rpx;
    line-height: 80rpx;
}
.content_view{
    width: 100%;
    height: 100%;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;
}

