<view class="dept_map_view"><block wx:if="{{lng&&lat}}"><map hidden="{{!(!isInputStatus)}}" class="dept_map" longitude="{{lng}}" latitude="{{lat}}" subkey="{{mapKey}}" show-location="true" scale="{{scale}}" layer-style="3" id="deptMapId" markers="{{makerList}}" data-event-opts="{{[['callouttap',[['bindCalloutTap',['$event']]]],['regionchange',[['bindRegionChange',['$event']]]],['end',[['bindRegionChange',['$event']]]],['begin',[['bindRegionChange',['$event']]]]]}}" bindcallouttap="__e" bindregionchange="__e" bindend="__e" bindbegin="__e"><cover-image class="back_location" src="{{locationImg}}" data-event-opts="{{[['tap',[['backLocation',['$event']]]]]}}" bindtap="__e"></cover-image><cover-view data-event-opts="{{[['tap',[['inputClick',['$event']]]]]}}" class="top_input" bindtap="__e"><cover-view class="top_city_name">{{cityName}}</cover-view><cover-view class="top_line"></cover-view><cover-image class="top_search_icon" src="{{searchIcon}}"></cover-image><cover-view hidden="{{!(!keyWord)}}" class="top_input_pla">请输入区域、门店名称搜索</cover-view><cover-view hidden="{{!(keyWord)}}" class="top_input_val">{{keyWord}}</cover-view><cover-image hidden="{{!(keyWord)}}" class="top_clear_icon" src="http://cdn.haofang.net/static/uuminiapp/search/clear.png" data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" bindtap="__e"></cover-image></cover-view></map></block></view>