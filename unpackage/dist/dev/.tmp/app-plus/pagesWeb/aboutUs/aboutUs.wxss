
.logo_view {
	width: 136rpx;
	height: 136rpx;
	overflow: hidden;
	margin: 150rpx auto 0;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	border-radius: 30rpx;
}
.logo_view image {
	width: 100%;
	height: 100%;
}
.logo_title {
	font-family: PingFang-SC-Bold;
	font-size: 36rpx;
	color: #101d36;
	line-height: 73rpx;
	text-align: center;
	font-weight: 600;
}
.about_cont {
	margin-top: 53rpx;
	padding: 0 93rpx;
	box-sizing: border-box;
	text-align: justify;
	color: #101d36;
	font-size: 30rpx;
}
.service_tel {
	font-size: 28rpx;
	color: #8e94a1;
	line-height: 56rpx;
	text-align: center;
	position: fixed;
	bottom: 65rpx;
	left: 50%;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
}

