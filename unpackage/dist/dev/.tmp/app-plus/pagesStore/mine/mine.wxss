.person-info{width: 670rpx;height: 190rpx;
    margin: 24rpx auto 0;
    padding: 0 30rpx;
    box-sizing: border-box;
    box-shadow:0px 6rpx 24rpx 0rpx rgba(0, 0, 0, 0.08);
    border-radius: 14rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    position: relative;
}
.person-info .name-box {line-height: 1;font-size: 46rpx;font-weight: bold;color: #000;-webkit-box-flex:1;-webkit-flex:1;flex:1;}
.person-info .name-box .name {}
.person-info .name-box .updata-userinfo {font-size: 24rpx;color: #999;margin-top: 21rpx;}
.person-info .name-box .updata-userinfo::after{content: '';margin-left: 9rpx;display: inline-block;width: 20rpx;height: 20rpx;
    background: url(http://cdn.haofang.net/static/uuminiapp/compStore/mine_update.png) center center no-repeat;
    background-size: cover;
}
.person-info .head-img {width: 130rpx;height: 130rpx;border-radius: 100%;overflow: hidden;
    background: url(https://zdzfapi.haofang.net/Public/wxApp/images/mine/fang_default.png) center center no-repeat;
    background-size: cover;
}
.person-info .head-img image {width: 100%;height: 100%;}
.person-info .login-up-msg-mask {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: none;
    border-color: #fff;
}
.person-info .login-up-msg-mask::after {
    border: 0;
}
