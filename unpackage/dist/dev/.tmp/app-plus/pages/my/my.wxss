  border: none;
  border-radius: 0;
  will-change: transform;
  padding: 10rpx 0 10rpx 30rpx;
}
.my_header_view::after {
  border: none;
  border-radius: 0;
}
button.img_view, .img_view {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  border: 2rpx solid #e2e2e2;
  overflow: hidden;
  box-sizing: border-box;
  margin: auto 20rpx auto 0;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
}
.img_view image {
  width: 100%;
  height: 100%;
}
.data_info {
  position: relative;
  width: 90%;
}
.up_top_info {
  margin-top: 8rpx;
