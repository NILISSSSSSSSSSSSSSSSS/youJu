/* 个人钱包 */
.wallet {
  background: #FFFFFF;
  box-sizing: border-box;
  padding: 0 30rpx 30rpx;
}
.wallet_title {
  position: relative;
  width: 100%;
  height: auto;
  line-height: 104rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  overflow: hidden;
}
.right_arrow {
  width: 13rpx;
  height: 24rpx;
  margin: auto 0;
}
.wallet_cont {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 170rpx;
  background-color: #ffffff;
  box-shadow: 0 4rpx 33rpx 2rpx rgba(55, 55, 55, 0.1);
  border-radius: 10rpx;
  box-sizing: border-box;
}
.wallet_item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  height: 100%;
}
.wallet_val_text {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  margin-right: 10rpx;
  position: relative;
  top: 2px;
  color: #333333;
  font-size: 44rpx;
}
.wallet_val_unit {
  font-size: 22rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.wallet_text {
  margin-top: 20rpx;
