<view class="message_view"><block wx:if="{{!chartList}}"><scroll-loading vue-id="1" isScrollIng="{{true}}" haveMoreData="{{true}}" bind:__l="__l"></scroll-loading></block><view><view data-event-opts="{{[['tap',[['goSysMsg',['$event']]]]]}}" class="f_r_s message_item" bindtap="__e"><view class="head_img_view"><image mode="aspectFill" src="http://cdn.haofang.net/static/uuminiapp/im/icon_systeminformation.png"></image><block wx:if="{{sysMsgItem.haveNewMsg}}"><view class="point_red"></view></block></view><view class="f_c_c message_cont"><view class="f_r_b message_top"><view class="massage_name">系统消息</view><view class="message_last_time">{{sysMsgItem.timeText||""}}</view></view><view class="f_r_b msg_text_view"><view class="msg_text">{{(sysMsgItem.contents&&sysMsgItem.contents.length>0?sysMsgItem.contents[0]:"")+''}}</view></view></view></view><block wx:for="{{chartList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getChartDetail',['$0',index],[[['chartList','',index]]]]]],['touchstart',[['touchStart',['$event',index,'$0'],[[['chartList','',index]]]]]],['touchend',[['touchEnd',['$event',index,'$0'],[[['chartList','',index]]]]]]]}}" class="{{['f_r_s message_item',(item.isShowBtn)?'message_left':'']}}" catchtap="__e" bindtouchstart="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['getChartDetail',['$0',index],[[['chartList','',index]]]]]]]}}" class="head_img_view" catchtap="__e"><image mode="aspectFill" src="{{item.headImg}}" data-event-opts="{{[['error',[['imgError',['$0',index],[[['chartList','',index]]]]]]]}}" binderror="__e"></image><block wx:if="{{item.unRead&&item.unRead>99}}"><view class="point">99+</view></block><block wx:if="{{item.unRead&&item.unRead<=99}}"><view class="point">{{item.unRead}}</view></block></view><view class="f_c_c message_cont"><view data-event-opts="{{[['tap',[['getChartDetail',['$0',index],[[['chartList','',index]]]]]]]}}" class="f_r_b message_top" catchtap="__e"><view class="f_r_s massage_name_view"><view class="massage_name">{{item.name}}</view><block wx:if="{{item.compName}}"><view class="massage_compName">{{item.compName}}</view></block></view><view class="message_last_time">{{item.lastTime}}</view></view><view data-event-opts="{{[['tap',[['getChartDetail',['$0',index],[[['chartList','',index]]]]]]]}}" class="f_r_b msg_text_view" catchtap="__e"><view class="msg_text">{{item.msg}}</view><block wx:if="{{item.isShield==1}}"><view class="ping_bi_icon"></view></block></view></view><view class="f_r_e right_btn_view"><view data-event-opts="{{[['tap',[['blockMsg',['$0',index],[[['chartList','',index]]]]]]]}}" class="ping_bi_btn" catchtap="__e">{{(item.isShield==1?"解除屏蔽":"屏蔽")+''}}</view><view data-event-opts="{{[['tap',[['delMsg',['$0',index],[[['chartList','',index]]]]]]]}}" class="del_btn" catchtap="__e">删除</view></view></view></block></view><view class="empty_view"></view><login-view vue-id="2" bind:__l="__l"></login-view></view>