<form report-submit data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="choose_city" style="{{'height:'+(pageHeight)+';'}}"><scroll-view class="choose_city_scroll" scroll-with-animation="{{scrollAnimate}}" enable-back-to-top scroll-into-view="{{scrollIntoId}}" scroll-y><view class="choose_city_cont"><view class="choose_title">当前定位城市</view><button class="gps_city f_r_s" form-type="submit" hover-class="none" data-event-opts="{{[['tap',[['cityBtn',['$0'],['gpsCityInfo']]]]]}}" bindtap="__e"><block wx:if="{{gpsCityInfo.cityName!='定位中...'}}"><image class="gps_city_icon" src="https://zdzfapi.haofang.net/PublicC/images/icon_mylocation.png"></image></block><view class="gps_city_text">{{gpsCityInfo.cityName}}</view></button><view class="choose_title">热门城市</view><view class="hot_city_list f_r_s"><block wx:for="{{hotCityList}}" wx:for-item="item" wx:for-index="hotIndex" wx:key="hotIndex"><block index="{{hotIndex}}"><button class="hot_city_item" form-type="submit" hover-class="none" data-event-opts="{{[['tap',[['cityBtn',['$0'],[[['hotCityList','',hotIndex]]]]]]]}}" bindtap="__e">{{item.cityNameLess}}</button></block></block></view><block wx:for="{{cityList}}" wx:for-item="item" wx:for-index="cityIndex" wx:key="cityIndex"><block index="{{cityIndex}}"><view class="choose_title" id="{{item.title.id}}">{{item.title.text}}</view><block wx:for="{{item.list}}" wx:for-item="info" wx:for-index="cityInfoIndex" wx:key="cityName"><block index="{{cityInfoIndex}}"><button class="city_item" form-type="submit" hover-class="none" data-event-opts="{{[['tap',[['cityBtn',['$0'],[[['cityList','',cityIndex],['list','cityName',info.cityName]]]]]]]}}" bindtap="__e">{{info.cityName}}</button></block></block></block></block></view></scroll-view><view class="letter_list f_c_c"><block wx:for="{{letterList}}" wx:for-item="item" wx:for-index="letterIndex" wx:key="letterIndex"><block index="{{letterIndex}}"><button class="f_r_c letter_item" form-type="submit" hover-class="none" id="{{item.id}}" data-event-opts="{{[['tap',[['letterBtn',['$0'],[[['letterList','',letterIndex]]]]]]]}}" bindtap="__e">{{item.text}}</button></block></block></view></view></form>