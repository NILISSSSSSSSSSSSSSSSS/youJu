<view class="m-list-ul"><block wx:for="{{lookList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['houseDetail',['$0'],[[['lookList','',index]]]]]]]}}" class="m-list-item" bindtap="__e"><text class="m-round"></text><text class="m-round-line"></text><block wx:if="{{item.lookSource!==1}}"><view class="u-item-image"><image src="{{item.thumbUrl}}" mode="aspectFill"></image><block wx:if="{{type!==1&&item.lookFlag!==1}}"><image class="u-item-label" src="http://cdn.haofang.net/static/uuminiapp/packageShare/takeAboutLook/no_house_icon.png"></image></block></view></block><view class="u-item-info"><view class="{{['u-info-build-name',type!==1&&item.lookFlag!==1?'not-look':'']}}">{{item.buildName}}</view><block wx:if="{{item.lookSource!==1}}"><view class="{{['u-info-build-info',type!==1&&item.lookFlag!==1?'not-look':'']}}"><text>{{(item.houseRoom?item.houseRoom+"室":"")+(item.houseWei?item.houseWei+"卫":"")+(item.houseHall?item.houseHall+"厅":"")}}</text><block wx:if="{{item.houseArea}}"><text>{{item.houseArea+"㎡"}}</text></block><block wx:if="{{item.regionName}}"><text>{{item.regionName}}</text></block></view></block><block wx:if="{{item.lookSource!==1}}"><view class="{{['u-info-build-price',type!==1&&item.lookFlag!==1?'not-look':'']}}"><block wx:if="{{item.houseTotalPrice}}"><text class="{{['u-total-price',type!==1&&item.lookFlag!==1?'not-look':'']}}">{{item.houseTotalPrice+''}}<text class="u-total-price-unit">{{item.caseType==3?"万":"元"}}</text></text></block><block wx:if="{{item.houseUnitPrice}}"><text class="{{['u-average-price',type!==1&&item.lookFlag!==1?'not-look':'']}}">{{item.houseUnitPrice+"元/㎡"}}</text></block></view></block></view></view></block></view>