<view data-event-opts="{{[['tap',[['erHouseDetail',['$0'],['item']]]]]}}" class="er_house_item f_r_s" bindtap="__e"><view class="er_house_img_view"><image class="er_house_img" mode="aspectFill" src="{{houseImgUrl}}" lazy-load></image><block wx:if="{{item.hasVr}}"><view class="vr_img"></view></block><block wx:if="{{!item.hasVr&&item.hasVideo}}"><view class="small_video_img"></view></block></view><view class="er_house_cont"><view class="er_house_title">{{item.houseTitle}}</view><block wx:if="{{item.houseUseage==1}}"><view class="er_house_des"><block wx:if="{{item.houseTing}}"><text class="er_house_ting">{{item.houseTing}}</text></block><block wx:if="{{!item.houseTing}}"><text class="er_house_ting">{{item.houseUseageCn}}</text></block><block wx:if="{{item.houseArea}}"><text class="er_house_area">{{item.houseArea+item.houseAreaUnit}}</text></block><text class="er_house_addr">{{item.houseName}}</text></view></block><block wx:else><view class="f_r_s er_house_des"><view class="er_house_addr">{{item.houseUseageCn}}</view></view></block><block wx:if="{{!(!item.trueHouse&&(!item.houseTags||item.houseTags.length==0))}}"><view class="er_house_tag_info f_r_s"><block wx:if="{{item.trueHouse}}"><view class="er_house_true"></view></block><block wx:for="{{item.houseTags}}" wx:for-item="tagInfo" wx:for-index="index" wx:key="index"><block><block wx:if="{{tagInfo=='房租月付'}}"><view class="er_house_tag f_r_c">{{tagInfo}}</view></block><block wx:else><block wx:if="{{tagInfo=='业主房源'}}"><view class="er_house_tag f_r_c">{{tagInfo}}</view></block><block wx:else><block wx:if="{{tagInfo&&tagInfo.length>1}}"><view class="er_house_tag f_r_c">{{tagInfo}}</view></block></block></block></block></block></view></block><block wx:if="{{item.houseTotalPrice&&item.houseTotalPrice!=0}}"><view class="er_house_price_view f_r_s"><view class="er_house_price main_color f_r_s"><view>{{item.houseTotalPrice}}</view><view style="font-weight:normal;">{{item.houseTotalPriceUnit}}</view></view><view class="er_house_unit_price">{{item.housePriceOfArea+item.housePriceOfAreaUnit}}</view></view></block><block wx:else><view class="er_house_price_view f_r_s"><view class="er_house_unit_price gray_color">价格面议</view></view></block></view><block wx:if="{{item.isTopHouse}}"><view class="isTopHouse_view">广告</view></block></view>