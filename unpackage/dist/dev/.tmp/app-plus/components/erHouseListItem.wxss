
.isTopHouse_view{
		position: absolute;
		bottom: 30rpx;
		right: 0;
		color: #c8c8c9;
		font-size: 22rpx;
		text-align: right;
}
.er_house_item{
        padding: 40rpx 0 40rpx 0;
        box-sizing: border-box;
        background: #FFFFFF;
        border-bottom: 2rpx solid #F5F5F5;
}
.er_house_img_view{
        position: relative;
        width:216rpx;
        height:164rpx;
        -webkit-flex-shrink: 0;
                flex-shrink: 0;
		background:#f8f8f8 url(http://cdn.haofang.net/static/wxPlusApp/yjk/house/house_list_default.png) no-repeat center center;
		background-size:cover;
		border-radius: 4rpx;
		box-sizing: border-box;
}
.er_house_img{
        width: 100%;
        height: 100%;
}
.vr_img{
		position: absolute;
		left: 20rpx;
		bottom: 10rpx;
		z-index: 20;
		background:url(http://cdn.haofang.net/static/uuminiapp/pageNewUi/house_list_sprites.png)  no-repeat;
		background-size: 179px 24px;
		height: 28px;
		width: 25px;
		background-position: -39px 0;
}
.big_video_img{
        position: absolute;
        top: 50%;
        left: 50%;
        -webkit-transform: translate(-50%, -50%);
                transform: translate(-50%, -50%);
        z-index: 20;
		background:url(http://cdn.haofang.net/static/uuminiapp/pageNewUi/house_list_sprites.png)  no-repeat;
		background-size: 179px 24px;
		height: 28px;
		width: 25px;
		background-position:-17px 0;
}
.small_video_img{
        position: absolute;
        left: 20rpx;
        bottom: 10rpx;
        z-index: 20;
		background:url(http://cdn.haofang.net/static/uuminiapp/pageNewUi/house_list_sprites.png)  no-repeat;
		background-size: 179px 24px;
		height: 28px;
		width: 25px;
		background-position:-15px 0;
}
.er_house_cont{
        padding-left: 30rpx;
        box-sizing: border-box;
        width: calc(100% - 216rpx);
}
.er_house_title{
		width:100%;
		font-size:32rpx;
		font-weight:bold;
		color:#101d36;
		line-height:42rpx;
		margin-bottom:12rpx;
		overflow:hidden;
		word-wrap:break-word;
		word-break:break-all;
		text-overflow:ellipsis;
		display:-webkit-box;
		-webkit-line-clamp:2;
		-webkit-box-orient:vertical;
		margin-top: -4rpx;
}
.er_house_des{
			height: 30rpx;
        font-size:26rpx;
        line-height: 30rpx;
        color:#101d37;
		font-size: 26rpx;
        /* margin-bottom:7upx; */
        text-align: left;
        max-width: 100%;
        text-overflow: ellipsis;
        white-space: nowrap;
        overflow: hidden;
		margin-bottom: 15rpx;
}
.er_house_des text{
		color:#101d37;
}
.er_house_des text::after{
		content: "|";
		font-size: 16rpx;
		color: #babfc5;
		margin-left: 10rpx;
		margin-right: 10rpx;
		position: relative;
		top: -2rpx;
}
.er_house_des text:last-child::after{
		content: ''
}
.er_house_ting{
        /* margin-right: 8upx; */
}
.er_house_area{
        /* margin-right: 8upx; */
        text-align: left;
        max-width: 6em;
        text-overflow: ellipsis;
        white-space: nowrap;
        overflow: hidden;
}
.er_house_tag_info{
			height: 30rpx;
		overflow: hidden;
		margin-bottom: 15rpx;
}
.er_house_tag_view{
		-webkit-flex-wrap: wrap;
		        flex-wrap: wrap;
}
.er_house_true {
		margin: auto 10rpx auto 0;
		-webkit-flex-shrink: 0;
		        flex-shrink: 0;
		background:url('http://cdn.haofang.net/static/wxPlusApp/yjk/home/true_house.png')  no-repeat center;
		background-size: contain;
		height: 30rpx;
		width: 96rpx;
}
.er_house_tag_info .er_house_tag:nth-of-type(4n+1){
        color: #18acf0;
        background: #ddf3fd;
}
.er_house_tag_info .er_house_tag:nth-of-type(4n+2){
        color: #ff9600;
        background: #fff1dc;
}
.er_house_tag_info .er_house_tag:nth-of-type(4n+3){
        color: #f3685f;
        background: #fde8e7;
}
.er_house_tag_info .er_house_tag:nth-of-type(4n+4){
        color: #1bc964;
        background: #ddf7e8;
}
.er_house_tag{
        white-space: nowrap;
		font-size:22rpx;
		display:-webkit-box;
		display:-webkit-flex;
		display:flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		color:#7991a6 !important;
		background:#edf0f3 !important;
		padding:0 6rpx;
		border-radius:2rpx;
		margin-right:10rpx;
		box-sizing: border-box;
}


.er_house_price_view{
		line-height:30rpx;
		height: 30rpx;
}
.er_house_price{
        font-size:30rpx;
        font-weight:bold;
        margin-right: 20rpx;
}
.er_house_unit_price{
        color: #8E94A1;
        font-weight:normal;
        font-size:22rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-pack: end;
		-webkit-justify-content: flex-end;
		        justify-content: flex-end;
		line-height: initial;
}
.er_house_addr{
		max-width: 100%;
		text-overflow: ellipsis;
		white-space: nowrap;
		overflow: hidden;
}

