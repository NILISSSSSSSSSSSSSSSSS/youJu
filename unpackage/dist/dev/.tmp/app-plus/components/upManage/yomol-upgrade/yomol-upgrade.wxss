
@-webkit-keyframes show{
0%{-webkit-transform: scale(0);transform: scale(0); background: rgba(255, 255, 255, 0.3);}
50%{-webkit-transform: scale(1.1);transform: scale(1.1); background: rgba(255, 255, 255, 0.6);}
100%{-webkit-transform: scale(1);transform: scale(1); background: rgba(255, 255, 255, 1);}
}
@keyframes show{
0%{-webkit-transform: scale(0);transform: scale(0); background: rgba(255, 255, 255, 0.3);}
50%{-webkit-transform: scale(1.1);transform: scale(1.1); background: rgba(255, 255, 255, 0.6);}
100%{-webkit-transform: scale(1);transform: scale(1); background: rgba(255, 255, 255, 1);}
}
@-webkit-keyframes showMark{
0%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0.1);}
100%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0.6);}
}
@keyframes showMark{
0%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0.1);}
100%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0.6);}
}
.show{
	-webkit-animation: show .4s ease-in-out forwards;
	        animation: show .4s ease-in-out forwards;
}
.show_mark{
	-webkit-animation: showMark .2s ease-in-out forwards;
	        animation: showMark .2s ease-in-out forwards;
}
.mark{
	position: fixed;
	top: 0;
	left: 0;
	min-height: 100vh;
	width: 100vw;
	background: rgba(0,0,0,0.1);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-flow: row nowrap;
	        flex-flow: row nowrap;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	z-index: 1000;
}
.view{
	padding-top: 147rpx;
	box-sizing: border-box;
	width: 80vw;
